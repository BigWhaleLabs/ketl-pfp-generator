    "axios": "^1.4.0",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "form-data": "^4.0.0",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.4.0",
    "koa-router": "^12.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.3",
    "mongoose-findorcreate": "^4.0.0",
    "node-fetch": "^3.3.1",
    "openai": "^4.16.1",
    "sharp": "0.32.5",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.0"
