    "@types/node": "^18.15.10",
    "@types/uuid": "^9.0.1",
    "eslint": "^8.36.0",
    "http-graceful-shutdown": "^3.1.13",
    "jest": "^29.5.0",
    "module-alias": "^2.2.2",
    "mongodb-memory-server": "^8.12.1",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.5",
